{
    "name": "bb2-building",
    "version": "1.0.0",
    "description": "# Running the app locally",
    "main": "index.js",
    "scripts": {
        "build": "npm run build:tailwind && npm run build:dx",
        "build:tailwind": "tailwindcss -m -i input.css -o public/tailwind.css",
        "build:dx": "dx build --release",
        "serve": "concurrently \"npm run serve:dx\" \"npm run serve:tailwind\"",
        "serve:tailwind": "tailwindcss --watch -i input.css -o public/tailwind.css",
        "serve:dx": "dx serve --hot-reload",
        "image": "squoosh-cli --webp '{\"quality\": 75}' --quant --output-dir public/images dump/asset/ExportedProject/Assets/Texture2D/sactx-0-8192x4096-Uncompressed-IconAtlas-eaf9cd50.png"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@squoosh/cli": "^0.7.3",
        "@tailwindcss/typography": "^0.5.10",
        "daisyui": "^4.6.0",
        "tailwindcss": "^3.4.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "run-script-os": "^1.1.6"
    }
}
